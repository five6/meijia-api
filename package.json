{
  "name": "meijia-server",
  "version": "0.0.1",
  "description": "",
  "author": "five6",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:dev": "env-cmd -f ./.env.dev nest build && npm run copy:files",
    "build:test": "env-cmd -f ./.env.test nest build && npm run copy:files",
    "build:stage": "env-cmd -f ./.env.stage nest build && npm run copy:files",
    "build:prod": "env-cmd -f ./.env.prod nest build && npm run copy:files",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "env-cmd -f ./.env.dev nest start --watch",
    "start:debug": "env-cmd -f ./.env.dev nest start --debug --watch",
    "start:test": "env-cmd -f ./.env.test nest start ",
    "start:stage": "env-cmd -f ./.env.stage nest start",
    "start:prod": "env-cmd -f ./.env.prod nest start",
    "start:prod2": "node dist/main",
    "copy:files": "copyfiles -V -E ./.env* ./dist/",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "6.4.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.8.1",
    "@nestjs/testing": "^7.0.9",
    "@nestjs/websockets": "^7.6.18",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.1",
    "cookie-parser": "^1.4.4",
    "crypto-random-string": "^3.2.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "lodash": "^4.17.21",
    "md5": "^2.2.1",
    "mongo-gridfs": "^1.1.0",
    "mongoose": "^5.12.14",
    "multer-gridfs-storage": "^4.0.2",
    "nestjs-redis": "^1.3.3",
    "nodemailer": "^6.6.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^4.1.3",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.0.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.13",
    "@types/multer-gridfs-storage": "^4.0.0",
    "@types/node": "^13.13.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
